<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--UIUrlTextView-->
    <declare-styleable name="UIUrlTextView">
        <attr name="autoLink" format="enum">
            <enum name="web" value="0" />
        </attr>
        <attr name="isLinkUnderline" format="boolean" />
        <attr name="linkTextColor" format="color" />
    </declare-styleable>


    <declare-styleable name="FinaceProgressBar">
        <attr name="uiPercantageIcon" format="reference" />
        <attr name="uiProgress" format="integer" />
        <attr name="uiDotResources" format="reference" />
        <attr name="isShowPercanage" format="boolean" />
    </declare-styleable>
    <!--折线图-->
    <declare-styleable name="LineChart">
        <attr name="lineColor" format="color" />
        <attr name="shadowColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="textBackground" format="reference" />
        <attr name="iseBezierCurve" format="boolean" />
    </declare-styleable>
</resources>